@model SangalTec.Models.Entities.Proveedor

@{ Layout = null; }

<form>
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <div class="col-md-12">
            <label asp-for="ProveedorId" class="control-label"></label>
            <br />
            @Model.ProveedorId
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-12">
            <label asp-for="Nombres" class="control-label"></label>
            <br />
            @Model.Nombres
        </div>
    </div>

    <div class="form-group">
    <div class="col-md-12">
        <label asp-for="Telefono" class="control-label"></label>
        <br />
        @Model.Telefono
    </div>
    </div>
    <div class="form-group">
        <div class="col-md-12">
            <label asp-for="Correo" class="control-label"></label>
            <br />
            @Model.Correo
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-12">
            <label asp-for="InsumoId" class="control-label"></label>
            <br />
            @Model.Insumo.Nombre
        </div>
    </div>

</form>
<script src="~/js/site.js"></script>
@await Html.PartialAsync("_ValidationScriptsPartial")